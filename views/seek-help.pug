extends layout

block content
  script(type='text/javascript', src='https://maps.googleapis.com/maps/api/js?key=#{apiKey}')
  script(src='https://unpkg.com/location-picker/dist/location-picker.min.js')
  h1 Seek Help
  #map(style='width: 100%; height: 800px;')
  script.
    var locationPicker = new locationPicker('map', {
    setCurrentPosition: true, // You can omit this, defaults to true
    }, {
        zoom: 15 // You can set any google map options here, zoom defaults to 15
    });
    